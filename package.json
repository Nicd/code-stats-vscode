{
    "name": "code-stats-vscode",
    "displayName": "Code::Stats",
    "description": "Code::Stats package for Visual Studio Code",
    "version": "1.0.7",
    "publisher": "juha-ristolainen",
    "icon": "logo.png",
    "repository": {
        "type": "github",
        "url": "https://github.com/riussi/code-stats-vscode"
    },
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Other"
    ],
    "maintainers": [
        "Juha Ristolainen"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/src/code-stats",
    "contributes": {
        "configuration": {
            "type": "object",
            "title": "code-stats-vscode",
            "properties": {
                "codestats.apikey": {
                    "type": "string",
                    "default": "",
                    "description": "Code::Stats API key for this machine."
                },
                "codestats.apiurl": {
                    "type": "string",
                    "default": "https://codestats.net/api/my/pulses",
                    "description": "Code::Stats API URL. Only change if you know what you are doing!"
                }
            }
        },
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "axios": "0.15.3"
    }
}
